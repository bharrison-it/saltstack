<VirtualHost *:80>
  DocumentRoot /var/www/html/observium/html/
  ServerName 192.168.11.7 
  CustomLog /var/www/html/observium/logs/access_log combined
  ErrorLog /var/www/html/observium/logs/error_log
  <Directory "/var/www/html/observium/html/">
   AllowOverride All
   Options FollowSymLinks MultiViews
  </Directory>
</VirtualHost>
